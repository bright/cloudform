  },
  "dependencies": {
    "@types/node": "^8.0.53",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "ts-node": "^5.0.1"
  }
}
