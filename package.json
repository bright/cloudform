{
  "name": "cloudform",
  "version": "1.0.4",
  "description": "TypeScript-based imperative way to define AWS CloudFormation templates",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bright/cloudform.git"
  },
  "scripts": {
